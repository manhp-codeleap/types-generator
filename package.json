{
  "name": "@musement/types-generator",
  "version": "0.0.1",
  "description": "A CLI to generate types definitions for flow and typescript",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "build": "tsc -p .",
    "build:watch": "tsc -p . --watch"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "cli",
    "generate-types"
  ],
  "bin": {
    "@musement/generate-types": "bin/index.js",
    "generate-types": "bin/index.js"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "axios": "^0.19.2",
    "camelize": "^1.0.0",
    "commander": "^4.1.1",
    "fp-ts": "^2.5.3",
    "inquirer": "^7.1.0",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.4",
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^13.9.1",
    "@types/prettier": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
